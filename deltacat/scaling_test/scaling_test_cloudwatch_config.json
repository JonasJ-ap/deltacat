{
   "agent":{
      "metrics_collection_interval":60,
      "run_as_user":"root"
   },
   "logs":{
      "logs_collected":{
         "files":{
            "collect_list":[
               {
                  "file_path":"/tmp/ray/session_*/logs/monitor.log",
                  "log_group_name":"ray_logs_autoscaler",
                  "log_stream_name":"{instance_id}"
               },
               {
                  "file_path":"/tmp/ray/session_*/logs/**.out",
                  "log_group_name":"ray_logs_out",
                  "log_stream_name":"{instance_id}"
               },
               {
                  "file_path":"/tmp/ray/session_*/logs/**.err",
                  "log_group_name":"ray_logs_err",
                  "log_stream_name":"{instance_id}"
               },
               {
                  "file_path":"/tmp/ray/session_*/logs/**.log",
                  "log_group_name":"ray_logs_log",
                  "log_stream_name":"{instance_id}"
               },
               {
                  "file_path":"/tmp/deltacat/var/output/logs/**.info.log*",
                  "log_group_name":"application_logs_info",
                  "log_stream_name":"{instance_id}"
               },
               {
                  "file_path":"/tmp/deltacat/var/output/logs/**.debug.log*",
                  "log_group_name":"application_logs_debug",
                  "log_stream_name":"{instance_id}"
               },
               {
                  "file_path":"/opt/amazon/andes/var/output/logs/application.info.log*",
                  "log_group_name":"sungate_java_logs_info",
                  "log_stream_name":"{instance_id}"
               },
               {
                  "file_path":"/opt/amazon/andes/var/output/logs/application.debug.log*",
                  "log_group_name":"sungate_java_logs_debug",
                  "log_stream_name":"{instance_id}"
               }
            ]
         }
      }
   },
   "metrics": {
     "namespace": "{cluster_name}-ray-CWAgent",
     "aggregation_dimensions": [
       [
         "InstanceId"
       ]
     ],
     "append_dimensions": {
       "AutoScalingGroupName": "${aws:AutoScalingGroupName}",
       "InstanceId": "${aws:InstanceId}"
     },
     "metrics_collected": {
       "collectd": {
         "metrics_aggregation_interval": 60
       },
       "cpu": {
         "measurement": [
           "usage_active",
           "usage_system",
           "usage_user",
           "usage_idle",
           "time_active",
           "time_system",
           "time_user",
           "time_idle"
         ],
         "resources": [
           "*"
         ]
       },
       "processes": {
         "measurement": [
           "processes_running",
           "processes_sleeping",
           "processes_zombies",
           "processes_dead",
           "processes_total"
         ],
         "metrics_collection_interval": 60,
         "resources": [
           "*"
         ]
       },
       "disk": {
         "measurement": [
           "disk_used_percent"
         ],
         "metrics_collection_interval": 60,
         "resources": [
           "/"
         ]
       },
       "mem": {
         "measurement": [
           "mem_used_percent"
         ],
         "metrics_collection_interval": 60,
         "resources": [
           "*"
         ]
       },
       "statsd": {
         "metrics_aggregation_interval": 60,
         "metrics_collection_interval": 10,
         "service_address": ":8125"
       }
     }
   }
}

